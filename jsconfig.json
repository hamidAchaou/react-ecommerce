{
    "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "@components/*": ["src/components/*"],
        "@pages/*": ["src/pages/*"],
        "@utils/*": ["src/utils/*"],
        "@assets/*": ["src/assets/*"],
        "@hooks/*": ["src/hooks/*"],
        "@styles/*": ["src/styles/*"]
      },
      "jsx": "react-jsx",
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    },
    "exclude": ["node_modules", "dist"]
  }
  